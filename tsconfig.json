{
  "version": "1.6.0",
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "listFiles": false
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
